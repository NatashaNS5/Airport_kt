<Window x:Class="Airport.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Airport"
        mc:Ignorable="d"
       Title="Регистрация" Height="300" Width="400">
    <StackPanel Margin="20">

        <TextBlock Text="Логин"/>
        <TextBox Name="LoginBox" Margin="0,5"/>

        <TextBlock Text="Пароль"/>
        <PasswordBox Name="PasswordBox" Margin="0,5"/>

        <Button Content="Зарегистрироваться" Click="Register_Click" Margin="0,10"/>
    </StackPanel>
</Window>
